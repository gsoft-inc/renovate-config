  "extends": [
      "github>gsoft-inc/renovate-config"
  ],
  "packageRules": [
    {
      "groupName": "microsoft",
      "matchPackageNames": [
        "/^[mM]icrosoft\\./",
        "/^[sS]ystem\\./"
